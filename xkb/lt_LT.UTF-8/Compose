















































# UTF-8  (Unicode) compose sequences
# This compose sequence map is for Lithuanian accented letters, which are in the private font area (needs a special font).
# These accented letters are not included in the Unicode standard.
# Here, the Lithuanian special accented "i" is entered by pressing "h".
# Made for Lithuanian Ratise (with dead keys) keymap ver. 2.1 by Algirdas Buckus <algirdasbuckus@gmail.com>
# Should work with other layouts (with dead keys) as well.

# Use the sequences from en_US.UTF-8 as the basis:
include "/usr/share/X11/locale/en_US.UTF-8/Compose"


<dead_grave> <U0068>    : "" Ue00c # LATIN SMALL LETTER I WITH GRAVE ACCENT

<dead_acute> <U0068>    : "" Ue00d # LATIN SMALL LETTER I WITH ACUTE ACCENT
<dead_acute> <U0104>    : "" Ue000 # LATIN CAPITAL LETTER A WITH OGONEK AND WITH ACUTE ACCENT
<dead_acute> <U0105>    : "" Ue001 # LATIN SMALL LETTER A WITH OGONEK AND WITH ACUTE ACCENT
<dead_acute> <U0116>    : "" Ue008 # LATIN CAPITAL LETTER E WITH DOT ABOVE AND WITH ACUTE ACCENT
<dead_acute> <U0117>    : "" Ue009 # LATIN SMALL LETTER E WITH DOT ABOVE AND WITH ACUTE ACCENT
<dead_acute> <U0118>    : "" Ue004 # LATIN CAPITAL LETTER E WITH OGONEK AND WITH ACUTE ACCENT
<dead_acute> <U0119>    : "" Ue005 # LATIN SMALL LETTER E WITH OGONEK AND WITH ACUTE ACCENT
<dead_acute> <U012e>    : "" Ue00f # LATIN CAPITAL LETTER I WITH OGONEK AND WITH ACUTE ACCENT
<dead_acute> <U012f>    : "" Ue010 # LATIN SMALL LETTER I WITH OGONEK AND WITH ACUTE ACCENT
<dead_acute> <U016a>    : "" Ue01f # LATIN CAPITAL LETTER U WITH MACRON AND WITH ACUTE ACCENT
<dead_acute> <U016b>    : "" Ue020 # LATIN SMALL LETTER U WITH MACRON AND WITH ACUTE ACCENT
<dead_acute> <U0172>    : "" Ue01b # LATIN CAPITAL LETTER U WITH OGONEK AND WITH ACUTE ACCENT
<dead_acute> <U0173>    : "" Ue01c # LATIN SMALL LETTER U WITH OGONEK AND WITH ACUTE ACCENT

<dead_tilde> <U004a>    : "" Ue013 # LATIN CAPITAL LETTER J WITH TILDE ACCENT
<dead_tilde> <U004c>    : "" Ue015 # LATIN CAPITAL LETTER L WITH TILDE ACCENT
<dead_tilde> <U004d>    : "" Ue017 # LATIN CAPITAL LETTER M WITH TILDE ACCENT
<dead_tilde> <U0052>    : "" Ue019 # LATIN SMALL LETTER R WITH TILDE ACCENT
<dead_tilde> <U0054>    : "⊨" U22a8 # TRUE
<dead_tilde> <U0068>    : "" Ue00e # LATIN SMALL LETTER I WITH TILDE ACCENT
<dead_tilde> <U006a>    : "" Ue014 # LATIN SMALL LETTER J WITH TILDE ACCENT
<dead_tilde> <U006c>    : "" Ue016 # LATIN SMALL LETTER L WITH TILDE ACCENT
<dead_tilde> <U006d>    : "" Ue018 # LATIN SMALL LETTER M WITH TILDE ACCENT
<dead_tilde> <U0072>    : "" Ue01a # LATIN SMALL LETTER R WITH TILDE ACCENT
<dead_tilde> <U0104>    : "" Ue002 # LATIN CAPITAL LETTER A WITH OGONEK AND WITH TILDE ACCENT
<dead_tilde> <U0105>    : "" Ue003 # LATIN SMALL LETTER A WITH OGONEK AND WITH TILDE ACCENT
<dead_tilde> <U0116>    : "" Ue00a # LATIN CAPITAL LETTER E WITH DOT ABOVE AND WITH TILDE ACCENT
<dead_tilde> <U0117>    : "" Ue00b # LATIN SMALL LETTER E WITH DOT ABOVE AND WITH TILDE ACCENT
<dead_tilde> <U0118>    : "" Ue006 # LATIN CAPITAL LETTER E WITH OGONEK AND WITH TILDE ACCENT
<dead_tilde> <U0119>    : "" Ue007 # LATIN SMALL LETTER E WITH OGONEK AND WITH TILDE ACCENT
<dead_tilde> <U012e>    : "" Ue011 # LATIN CAPITAL LETTER I WITH OGONEK AND WITH TILDE ACCENT
<dead_tilde> <U012f>    : "" Ue012 # LATIN SMALL LETTER I WITH OGONEK AND WITH TILDE ACCENT
<dead_tilde> <U016a>    : "" Ue021 # LATIN CAPITAL LETTER U WITH MACRON AND WITH TILDE ACCENT
<dead_tilde> <U016b>    : "" Ue022 # LATIN SMALL LETTER U WITH MACRON AND WITH TILDE ACCENT
<dead_tilde> <U0172>    : "" Ue01d # LATIN CAPITAL LETTER U WITH OGONEK AND WITH TILDE ACCENT
<dead_tilde> <U0173>    : "" Ue01e # LATIN SMALL LETTER U WITH OGONEK AND WITH TILDE ACCENT


