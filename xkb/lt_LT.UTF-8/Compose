















































# UTF-8  (Unicode) compose sequences
# This compose sequence map is for Lithuanian accented letters (JjLlMmRrĖėŪūiĄąĘęĮįŲų), which are in the private font area (needs a special font).
# These accented letters are not included in the Unicode standard.
# Here, the Lithuanian special accented "i" is entered by pressing "h".
# Made for Lithuanian Ratise (with dead keys) keymap by Algirdas Buckus <algirdasbuckus@gmail.com>
# Should work with other layouts (with dead keys) as well.

# Use the sequences from en_US.UTF-8 as the basis:
include "/usr/share/X11/locale/en_US.UTF-8/Compose"



<dead_grave> <h>    : "" Ue00c # LATIN SMALL LETTER I WITH GRAVE ACCENT

<dead_acute> <h>    : "" Ue00d # LATIN SMALL LETTER I WITH ACUTE ACCENT
<dead_acute> <Aogonek>    : "" Ue000 # LATIN CAPITAL LETTER A WITH OGONEK AND WITH ACUTE ACCENT
<dead_acute> <aogonek>    : "" Ue001 # LATIN SMALL LETTER A WITH OGONEK AND WITH ACUTE ACCENT
<dead_acute> <Eabovedot>    : "" Ue008 # LATIN CAPITAL LETTER E WITH DOT ABOVE AND WITH ACUTE ACCENT
<dead_acute> <eabovedot>    : "" Ue009 # LATIN SMALL LETTER E WITH DOT ABOVE AND WITH ACUTE ACCENT
<dead_acute> <Eogonek>    : "" Ue004 # LATIN CAPITAL LETTER E WITH OGONEK AND WITH ACUTE ACCENT
<dead_acute> <eogonek>    : "" Ue005 # LATIN SMALL LETTER E WITH OGONEK AND WITH ACUTE ACCENT
<dead_acute> <Iogonek>    : "" Ue00f # LATIN CAPITAL LETTER I WITH OGONEK AND WITH ACUTE ACCENT
<dead_acute> <iogonek>    : "" Ue010 # LATIN SMALL LETTER I WITH OGONEK AND WITH ACUTE ACCENT
<dead_acute> <Umacron>    : "" Ue01f # LATIN CAPITAL LETTER U WITH MACRON AND WITH ACUTE ACCENT
<dead_acute> <umacron>    : "" Ue020 # LATIN SMALL LETTER U WITH MACRON AND WITH ACUTE ACCENT
<dead_acute> <Uogonek>    : "" Ue01b # LATIN CAPITAL LETTER U WITH OGONEK AND WITH ACUTE ACCENT
<dead_acute> <uogonek>    : "" Ue01c # LATIN SMALL LETTER U WITH OGONEK AND WITH ACUTE ACCENT

<dead_tilde> <J>    : "" Ue013 # LATIN CAPITAL LETTER J WITH TILDE ACCENT
<dead_tilde> <L>    : "" Ue015 # LATIN CAPITAL LETTER L WITH TILDE ACCENT
<dead_tilde> <M>    : "" Ue017 # LATIN CAPITAL LETTER M WITH TILDE ACCENT
<dead_tilde> <R>    : "" Ue019 # LATIN CAPITAL LETTER R WITH TILDE ACCENT
<dead_tilde> <h>    : "" Ue00e # LATIN SMALL LETTER I WITH TILDE ACCENT
<dead_tilde> <j>    : "" Ue014 # LATIN SMALL LETTER J WITH TILDE ACCENT
<dead_tilde> <l>    : "" Ue016 # LATIN SMALL LETTER L WITH TILDE ACCENT
<dead_tilde> <m>    : "" Ue018 # LATIN SMALL LETTER M WITH TILDE ACCENT
<dead_tilde> <r>    : "" Ue01a # LATIN SMALL LETTER R WITH TILDE ACCENT
<dead_tilde> <Aogonek>    : "" Ue002 # LATIN CAPITAL LETTER A WITH OGONEK AND WITH TILDE ACCENT
<dead_tilde> <aogonek>    : "" Ue003 # LATIN SMALL LETTER A WITH OGONEK AND WITH TILDE ACCENT
<dead_tilde> <Eabovedot>    : "" Ue00a # LATIN CAPITAL LETTER E WITH DOT ABOVE AND WITH TILDE ACCENT
<dead_tilde> <eabovedot>    : "" Ue00b # LATIN SMALL LETTER E WITH DOT ABOVE AND WITH TILDE ACCENT
<dead_tilde> <Eogonek>    : "" Ue006 # LATIN CAPITAL LETTER E WITH OGONEK AND WITH TILDE ACCENT
<dead_tilde> <eogonek>    : "" Ue007 # LATIN SMALL LETTER E WITH OGONEK AND WITH TILDE ACCENT
<dead_tilde> <Iogonek>    : "" Ue011 # LATIN CAPITAL LETTER I WITH OGONEK AND WITH TILDE ACCENT
<dead_tilde> <iogonek>    : "" Ue012 # LATIN SMALL LETTER I WITH OGONEK AND WITH TILDE ACCENT
<dead_tilde> <Umacron>    : "" Ue021 # LATIN CAPITAL LETTER U WITH MACRON AND WITH TILDE ACCENT
<dead_tilde> <umacron>    : "" Ue022 # LATIN SMALL LETTER U WITH MACRON AND WITH TILDE ACCENT
<dead_tilde> <Uogonek>    : "" Ue01d # LATIN CAPITAL LETTER U WITH OGONEK AND WITH TILDE ACCENT
<dead_tilde> <uogonek>    : "" Ue01e # LATIN SMALL LETTER U WITH OGONEK AND WITH TILDE ACCENT


